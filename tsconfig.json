{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "outDir": "target",
        "resolveJsonModule": true,
        "sourceMap": true,
        "strictNullChecks": false,
        "target": "esnext"
    },
    "include": [
        "src/**/*"
    ]
}
